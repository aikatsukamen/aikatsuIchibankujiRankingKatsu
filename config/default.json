{
  "kkt": {
    "token": "",
    "visibility": "public"
  },
  "log4js": {
    "appenders": {
      "system": {
        "type": "file",
        "filename": "./log/system.log",
        "maxLogSize": 10485760,
        "backups": 1
      },
      "access": {
        "type": "file",
        "filename": "./log/access.log",
        "maxLogSize": 10485760,
        "backups": 1
      },
      "console": {
        "type": "console"
      }
    },
    "categories": {
      "default": {
        "appenders": ["system"],
        "level": "debug"
      },
      "access": {
        "appenders": ["access"],
        "level": "debug"
      },
      "console": {
        "appenders": ["console"],
        "level": "debug"
      }
    }
  }
}
